		"jsonwebtoken": "^9.0.2",
		"swagger-ui-express": "^5.0.1"
	},
	"devDependencies": {
		"@babel/register": "^7.24.1",
		"chai": "^4.5.0",
		"mocha": "^10.8.2",
		"mochawesome": "^7.1.3",
		"nodemon": "^3.0.3",
		"sinon": "^17.0.1",
